{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 280,
    "height": 340,
    "data": {"url": "https://raw.githubusercontent.com/anguyen-mpt/fit3179-datavis2/main/data/Top_10_SEIFA.csv"},
    "encoding": {
      "y": {
        "field": "LGA", 
        "type": "nominal", 
        "sort": "-x"},
      "x": {
        "field": "SEIFA Score", 
        "type": "quantitative", 
        "scale": {"domain": [0, 950]},
        "title": "IRSD score"}
    },
    "layer": [{
      "mark": "bar"
    }, {
      "mark": {
        "type": "text",
        "align": "left",
        "baseline": "middle",
        "dx": 3
      },
      "encoding": {
        "text": {
          "field": "SEIFA Score", 
          "type": "quantitative", 
          "format": ".1f"}
      }
    }]
  }}
