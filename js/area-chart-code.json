{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 100, 
    "height": 200,
    "title": "Breakdown of social competency development of Victorian children from 2009 to 2021",
    "data": {"url": "https://raw.githubusercontent.com/anguyen-mpt/fit3179-datavis2/main/data/vic_social_competence_2021.csv"},
    "mark": "area",
    "encoding": {
      "x": {
        "timeUnit": "year", "field": "Year", "title": "Year",
        "axis": {"format": "%Y"}
      },
      "y": {
        "aggregate": "sum", "field": "Proportion of children", "title": "Proportion of children (%)"
      },
      "color": {
        "field": "Level of development",
        "scale": {"scheme": "category10"}
      }
    }
  }
